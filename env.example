# RedFin API 환경 변수 설정

# 백엔드 설정 (FILE 또는 MONGO)
BACKEND=FILE

# 파일 백엔드 설정
NEWS_FILE=src/redfin_api/ai_news.jsonl

# MongoDB 백엔드 설정 (선택사항)
MONGO_URI=mongodb://localhost:27017
MONGO_DB=redfin
MONGO_COL=news

# API 설정
API_HOST=0.0.0.0
API_PORT=8000
API_RELOAD=false

# CORS 설정
CORS_ORIGINS=*

# 로깅 설정 (선택사항)
LOG_LEVEL=INFO
